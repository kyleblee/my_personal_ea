<section class="info-sections">

  <div class="main-wrappers interaction-wrapper">
    <h3>Delete?</h3>
    <p class="list-info">Are you sure you want to delete your account?</p>

    <div class="user-show-div list-divs">
      <form action="/users/<%=current_user.id%>" method="post">
        <input type="hidden" name="_method" value="delete">
        <input class="users-show-buttons buttons delete-buttons" type="submit" value="Yes">
      </form>
      <a href="/users/home"><button class="users-show-buttons buttons delete-buttons">No</button></a>
    </div>
  </div>
</section>
